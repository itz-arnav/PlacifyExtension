{
  "manifest_version": 3,
  "name": "Placify",
  "version": "1.0",
  "description": "Your one-stop solution for placements",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "128.png",
      "48": "128.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "128.png",
    "48": "128.png",
    "128": "128.png"
  },
  "permissions": [
    "tabs"
  ]
}
